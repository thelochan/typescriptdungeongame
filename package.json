  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mandshaw/dungeon-crawler-ts.git"
  },
  "homepage": "https://github.com/mandshaw/dungeon-crawler-ts.git",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "minimist": ">=1.2.2",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "typescript": "^3.8.3"
